        "command": "curl -s 'https://official-joke-api.appspot.com/random_joke' | sed -n 's/.*\"setup\":\"\\([^\"]*\\)\",\"punchline\":\"\\([^\"]*\\)\".*/\\1 \\2/p'"
    }
